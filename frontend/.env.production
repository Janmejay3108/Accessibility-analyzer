# Production environment variables for React frontend
# API will use the same domain as the frontend (served by Express)
# Empty string means use same domain (/api)
REACT_APP_API_BASE_URL=
REACT_APP_API_TIMEOUT=30000

# Firebase Web Configuration (replace with your actual config)
REACT_APP_FIREBASE_API_KEY=your-web-api-key
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-firebase-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
REACT_APP_FIREBASE_APP_ID=your-app-id
